<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.zucc.paperManageSys.Entity.UserEntity" table="user" schema="papermanagasystem">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(45)" length="45" not-null="false"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="jobtitle">
            <column name="jobtitle" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="creatTime">
            <column name="creat_time" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="editTime">
            <column name="edit_time" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="salt">
            <column name="salt" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>