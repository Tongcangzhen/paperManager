<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.zucc.paperManageSys.Entity.PaperEntity" table="paper" schema="papermanagasystem">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="papername">
            <column name="papername" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="papertype">
            <column name="papertype" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="teacherId">
            <column name="teacher_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="check">
            <column name="check" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="adminId">
            <column name="admin_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="creatTime">
            <column name="creat_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="editTime">
            <column name="edit_time" sql-type="datetime" not-null="true"/>
        </property>
        <property name="paperUrl">
            <column name="paper_url" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>